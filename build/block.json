{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/guten-accordion",
  "version": "0.1.0",
  "title": "Accordeon",
  "category": "text",
  "icon": "flag",
  "description": "Accordion for Guttenberg",
  "attributes": {
    "message": {
      "type": "string",
      "source": "text",
      "selector": "div"
    },
    "isOpen": {
      "type": "boolean",
      "default": "false"
    },
    "title": {
      "type": "string",
      "default": "Title"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "guten-accordion",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}